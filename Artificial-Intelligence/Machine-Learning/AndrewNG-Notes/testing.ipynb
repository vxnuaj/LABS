    "\n",
    "print(X)\n",
    "\n",
    "avg1 = np.mean(X)\n",
    "avg2 = 1/len(X.flatten()) * np.sum(X)\n",
    "\n",
    "print(len(X))\n",
    "\n",
    "print(avg1)\n",
    "print(avg2)"
   ]
