   "source": [
    "import numpy as np\n",
    "\n",
    "\n",
    "v = np.random.randn(3, 1)\n",
    "v_norm = np.linalg.norm(v, ord = 2)\n",
    "u = v / v_norm\n",
    "\n",
    "V = np.random.randn(3, 30)\n",
    "V_norm = np.linalg.norm(V, ord = 2)\n",
    "U = V / V_norm\n",
    "\n",
    "ans = np.abs(np.dot(u.T, U))\n",
    "\n",
    "print(np.mean(ans))"
   ]
  }
 ],
